<head>
	<meta charset="utf-8">
	<meta http-equiv="X-UA-Compatible" content="IE=edge">
	<title>
		{{#if title}}
		{{title}}
		{{else}}
		Default HandleBars Page Title
		{{/if}}
	</title>
	<meta name="description" content="">
	<meta name="viewport" content="width=device-width, initial-scale=1">
	{{!-- <link rel="stylesheet" href="main.css"> --}}
	{{#> head-block}}
	{{!-- Custom <head> content per page could be added. --}}
	{{/head-block}}
</head>

<body>
	{{#> hero-block}}
	{{!-- Hero content goes here. --}}
	<p>Default hero block content.</p>
	<ul>
		<li><a href="/page-one.html">Page One</a></li>
		<li><a href="/page-two.html">Page Two</a></li>
	</ul>
	{{/hero-block}}

	{{#> footer-block}}
	{{!--
      The `organisms/footer` partial is the default content,
      but can be overridden.
    --}}
	{{> organisms/footer }}
	{{/footer-block}}


	{{#> scripts-block}}
	{{!-- Custom scripts per page can be added. --}}
	{{/scripts-block}}
</body>

</html>